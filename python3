# 5.#Types of operators
# #arithmetic operator
a=5
b=2
print(a+b)
print(a-b)
print(a*b)
print(a/b)
print(a%b)  #reminder
print(a**b) #a^b

6.#relational operator
a=50
b=20
print(a==b) #false
print(a!=b) #true
print(a>=b) #true
print(a<=b) #false
print(a<b)  #True

# 7.#assignment operators
num=10
num=num+10  #10+10=20
print("num:",num)

num=10
num+=10
print("num:",num)

num=20
num-=20
print("num:",num)

num=10
num*=10
print("num:",num)

num=20
num/=5
print("num:",num)

num=5
num%=2
print("num:",num)


#logical operator
a=50
b=30
print(not False)
print(not True)

val1=True
val2=True
print("and operator:",val1 and val2)
print("OR operator:",(a==b) or (a>b))






